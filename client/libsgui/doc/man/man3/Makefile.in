# targets for this directory
# ---------------------------------------------------------------------------
@BUILD_DOC@all: Makefile $(DOXYFILE) $(DOCS)
@NO_BUILD_DOC@all: Makefile 
install: install-data

# install config
# ---------------------------------------------------------------------------
@BUILD_DOC@man_DATA   = $(DOCS)
@BUILD_DOC@man_PREFIX = /man3

# dist config
# ---------------------------------------------------------------------------
DISTFILES += $(DOCS)

# clean config
# ---------------------------------------------------------------------------
CLEANFILES += $(DOCS)

# build config
# ---------------------------------------------------------------------------
$(DOCS): $(DOXYFILE)
	@$(ECHO_DOXYGEN)
	@QUIET@$(RM) $(DOCS)
	@QUIET@cd $(DOXYPATH) && \
  (cat Doxyfile && \
   echo "GENERATE_MAN = YES" && \
   echo "GENERATE_HTML = NO") | \
   $(DOXYGEN) - $(REDIR)
	@QUIET@grep '^\.so' *.3 | grep -v '^sg' | sed 's/:.*//' | xargs rm -f
# include global make targets
# ---------------------------------------------------------------------------
include $(top_builddir)build.mk

